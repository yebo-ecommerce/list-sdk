    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.22.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
